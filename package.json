{
	"name": "paul-reid-photography",
	"version": "1.0.0",
	"private": true,
	"description": "TODO",
	"author": "Paul Reid",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"@tailwindcss/postcss7-compat": "^2.0.2",
		"@tailwindcss/typography": "^0.4.1",
		"autoprefixer": "^9.8.8",
		"core-js": "^3.6.5",
		"cssnano": "^4.1.11",
		"postcss": "^7",
		"postcss-advanced-variables": "^3.0.1",
		"postcss-cli": "^7.1.2",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.3",
		"postcss-scss": "^3.0.5",
		"postcss-sort-media-queries": "^1.31.21",
		"register-service-worker": "^1.7.1",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
		"vue": "^2.6.11",
		"vue-class-component": "^7.2.3",
		"vue-property-decorator": "^9.1.2",
		"vue-router": "^3.5.3"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"@vue/cli-plugin-babel": "~4.5.15",
		"@vue/cli-plugin-eslint": "~4.5.15",
		"@vue/cli-plugin-pwa": "~4.5.0",
		"@vue/cli-plugin-router": "~4.5.15",
		"@vue/cli-plugin-typescript": "~4.5.0",
		"@vue/cli-service": "~4.5.15",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-typescript": "^7.0.0",
		"eslint": "^6.7.2",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-vue": "^6.2.2",
		"prettier": "^2.2.1",
		"prettier-plugin-ini": "^0.3.1",
		"prettier-plugin-jsdoc": "^0.3.24",
		"typescript": "~4.4.4",
		"vue-cli-plugin-tailwind": "~2.0.6",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^5.64.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended",
			"@vue/typescript/recommended",
			"@vue/prettier",
			"@vue/prettier/@typescript-eslint"
		],
		"parserOptions": {
			"ecmaVersion": 2020
		},
		"rules": {
			"prettier/prettier": 0,
			"@typescript-eslint/no-namespace":0
		}
	},
	"prettier": {
		"iniSpaceAroundEquals": true,
		"printWidth": 100,
		"proseWrap": "always",
		"quoteProps": "consistent",
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true,
		"jsdocDescriptionWithDot": true,
		"jsdocPrintWidth": 100,
		"jsdocSeparateReturnsFromParam": true,
		"bracketSameLine": false,
		"vueIndentScriptAndStyle": true,
		"overrides": [
			{
				"files": [
					"**.md"
				],
				"options": {
					"trailingComma": "es5",
					"proseWrap": "never",
					"parser": "markdown"
				}
			},
			{
				"files": [
					"**.svg",
					"**.html",
					"**.xml"
				],
				"options": {
					"trailingComma": "none",
					"parser": "html",
					"bracketSameLine": true
				}
			},
			{
				"files": [
					"**.sass",
					"**.scss",
					"**.css"
				],
				"options": {
					"parser": "scss"
				}
			},
			{
				"files": [
					"**.env",
					"**.replit",
					"**/.tx/config"
				],
				"options": {
					"parser": "ini"
				}
			},
			{
				"files": [
					"**.json",
					"**.map"
				],
				"options": {
					"parser": "json"
				}
			},
			{
				"files": [
					"**.code-workspace",
					".code-snippets",
					"**/settings.json",
					"**/launch.json",
					"**/extensions.json",
					"**.jsonc",
					"**.eslintrc",
					"**.eslintrc.json",
					"jsconfig.json"
				],
				"options": {
					"parser": "json5",
					"quoteProps": "preserve",
					"trailingComma": "none"
				}
			}
		],
		"plugins": [
			"prettier-plugin-jsdoc",
			"prettier-plugin-ini"
		]
	},
	"browserslist": [
		"last 1 ios_saf major versions",
		"last 1 safari major versions",
		"> 0.5% and not ie > 0 and not op_mini > 0",
		"unreleased versions"
	],
	"bugs": {
		"url": "https://github.com/RedGuy12/paul-reid-photography/issues/new",
		"email": "paul@reid-family.org"
	},
	"gitHooks": {
		"pre-commit": "# todo lint-staged"
	},
	"homepage": "https://paul-reid.net/",
	"keywords": ["#todo"],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RedGuy12/paul-reid-photography.git"
	}
}
